{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "semi": [0],
    "comma-dangle": [0],
    "linebreak-style": [0]
  }
}
